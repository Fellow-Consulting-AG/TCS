			private function sortNumberic(obj1:Object, obj2:Object):int
			{
				//#1253 CRO
				if(entity == Database.opportunityDao.entity && sortedColumn == "Revenue") sortedColumn= "Revenue_sorting";
				return ObjectUtil.numericCompare(parseFloat(obj1[sortedColumn]),parseFloat(obj2[sortedColumn]));
			}
			private function sortDateTime(obj1:Object, obj2:Object):int
			{
				var dateFormat:Object = DateUtils.getCurrentUserDatePattern();
				var d1:Number = DateUtils.parse(obj1[sortedColumn], dateFormat.dateFormat + " " + dateFormat.timeFormat).getTime();
				var d2:Number = DateUtils.parse(obj2[sortedColumn], dateFormat.dateFormat + " " + dateFormat.timeFormat).getTime();
				if(d1 < d2) {
					return -1;
				} else if(d1 == d2) {
					return 0;
				}
				return 1;
			}
			private function sortDate(obj1:Object, obj2:Object):int
			{
				var dateFormat:Object = DateUtils.getCurrentUserDatePattern();
				var d1:Number = DateUtils.parse(obj1[sortedColumn], dateFormat.dateFormat).getTime();
				var d2:Number = DateUtils.parse(obj2[sortedColumn], dateFormat.dateFormat).getTime();
				if(d1 < d2) {
					return -1;
				} else if(d1 == d2) {
					return 0;
				}
				return 1;
			}

